<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
        <Cell><Data ss:Type="String">Code</Data></Cell>
        <Cell><Data ss:Type="String">Role</Data></Cell>
        <Cell><Data ss:Type="String">Account</Data></Cell>
        <Cell><Data ss:Type="String">Activation Time</Data></Cell>
        <Cell><Data ss:Type="String">Duration</Data></Cell>
      </Row>
    <% @account_codes.each do |item| %>
      <Row>
        <Cell><Data ss:Type="String"><%= item.code %></Data></Cell>
        <Cell><Data ss:Type="String"><%= item.role %></Data></Cell>
        <Cell><Data ss:Type="String"><%= item.userid || '未使用' %></Data></Cell>
        <Cell><Data ss:Type="String"><%= item.activation_time %></Data></Cell>
        <Cell><Data ss:Type="String"><%= item.duration %></Data></Cell>
      </Row>
    <% end %>
    </Table>
  </Worksheet>
</Workbook>